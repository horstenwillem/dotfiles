{
  "log": {
    "prefix": "log",
    "body": [
      "console.log($1);"
    ],
    "description": "console.log()"
  },
  "Print to console": {
    "prefix": "clg",
    "body": [
      "console.log('$1', $1);",
      "$2"
    ],
    "description": "Log output to console"
  },
  "describe": {
    "prefix": "desc",
    "body": [
      "describe('${1:what?}', () => {",
      "\t${0:}",
      "})"
    ],
    "description": "Mocha BDD: describe()"
  },
  "it": {
    "prefix": "ii",
    "body": [
      "it(\"${1:should do what...}\", (${2}) => {",
      "\t${0:}",
      "})"
    ],
    "description": "Mocha BDD: it()"
  },
  "JS & TS Stub": {
    "prefix": "stub",
    "body": [
      "/**",
      "* ${TM_FILENAME}",
      "* ${TM_DIRECTORY}",
      "*",
      "* Copyright (c) 2018 iCapps. All rights reserved.",
      "*/"
    ],
    "description": "Insert Copyright Statement"
  }
}